{
  "name": "doro-clicker",
  "version": "1.0.0",
  "scripts": {
    "test:unit": "jest unit",
    "test:unit:watch": "jest --watchAll",
    "test:unit:coverage": "jest unit --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/plugin-transform-modules-commonjs": "^7.26.3",
    "@babel/preset-env": "^7.23.2",
    "@playwright/test": "^1.51.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@types/node": "^22.14.1",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src/scripts"
    ],
    "testMatch": [
      "<rootDir>/test/unit/**/*.test.js",
      "**/?(*.)+(spec|test).js"
    ],
    "roots": [
      "<rootDir>"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "main": "playwright.config.js",
  "directories": {
    "test": "test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}
